#docker command to launch container for tv tracking

docker run --privileged -v /home/sp:/home/sp -v /dev:/dev -v /etc/localtime:/etc/localtime:ro --name 'tv-detect-final' -dit --restart unless-stopped -h `hostname` -it tv-detect-final.tar:latest /bin/bash

